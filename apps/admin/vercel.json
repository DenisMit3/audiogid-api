{
    "installCommand": "pnpm install --frozen-lockfile",
    "buildCommand": "pnpm build",
    "outputDirectory": ".next",
    "framework": "nextjs",
    "rewrites": [
        {
            "source": "/api/proxy/:path*",
            "destination": "https://api.audiogid.app/v1/:path*"
        }
    ]
}